<div class="container">
  <%#= render 'memos/error', model: @memo %>
  <div class="row">
    <div class="memo-new-box col-lg12 col-md12 col-12">
      <div class="new-box">
        <h1 class="new-title">メモ作成</h1>
        <%= form_with model: @memo, url:'/memos', local:true do |f| %>
        <h4 class="new-box">タイトル</h4>
        <%= f.text_field :title %>
        <h4 class="new-box">メモ</h4>
        <%= f.text_area :body %>
        <p><%= f.label :tag_names, "タグ" %></p>
        <p><%= f.text_field :tag_names %></p>
        <div>
          <%= f.submit '投稿' %>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>